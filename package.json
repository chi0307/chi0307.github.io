{
  "name": "website-new",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clear": "sh clear.sh",
    "generate-pages": "pnpm run clear && sh ./generate-pages.sh",
    "build": "pnpm run generate-pages && run-p check \"build-only {@}\" -- && pnpm run postbuild",
    "postbuild": "mkdir dist/.github && mkdir dist/.github/workflows && cp .github/workflows/update-dependencies.yml ./dist/.github/workflows/",
    "preview": "vite preview",
    "build-only": "vite build",
    "format": "pnpm run \"/^format:.*/\"",
    "format:lint": "eslint . --fix",
    "format:prettier": "prettier --write src/",
    "check": "pnpm run \"/^check:.*/\"",
    "check:build": "vue-tsc --build --force",
    "check:lint": "eslint .",
    "deploy": "gh-pages -d dist -m \"deploy($(git rev-parse --abbrev-ref HEAD)): $(git rev-parse HEAD)\""
  },
  "dependencies": {
    "@unocss/reset": "^0.64.1",
    "axios": "^1.7.8",
    "date-fns": "^4.1.0",
    "pinia": "^2.2.8",
    "typia": "^6.12.2",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.16.0",
    "@fortawesome/fontawesome-free": "^6.7.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia@^1.0.7",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@unocss/eslint-plugin": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.3",
    "eslint-plugin-perfectionist": "^4.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "gh-pages": "^6.2.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.16.0",
    "unocss": "^0.64.1",
    "vite": "^6.0.1",
    "vite-plugin-pwa": "^0.21.1",
    "vue-tsc": "^2.1.10"
  }
}
