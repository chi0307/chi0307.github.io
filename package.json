{
  "name": "website-new",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "clear": "sh clear.sh",
    "generate-pages": "pnpm run clear && sh ./generate-pages.sh",
    "build": "pnpm run generate-pages && run-p check test \"build-only {@}\" -- && pnpm run postbuild",
    "postbuild": "mkdir dist/.github && mkdir dist/.github/workflows && cp .github/workflows/update-dependencies.yml ./dist/.github/workflows/",
    "preview": "vite preview",
    "build-only": "vite build",
    "format": "pnpm run \"/^format:.*/\"",
    "format:lint": "eslint . --fix",
    "format:prettier": "prettier --write src/",
    "check": "pnpm run \"/^check:.*/\"",
    "check:build": "vue-tsc --build --force",
    "check:lint": "eslint .",
    "deploy": "gh-pages -d dist -m \"deploy($(git rev-parse --abbrev-ref HEAD)): $(git rev-parse HEAD)\"",
    "test": "vitest --run",
    "upgrade-packages": "ncu --doctor -u --doctorInstall \"pnpm install\" --doctorTest \"pnpm run-p check test\" --peer --target semver"
  },
  "dependencies": {
    "@chi0307/axios-wrapper": "^2.0.0",
    "@unocss/reset": "^66.4.2",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.3",
    "typia": "^9.7.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.5",
    "workbox-precaching": "^7.3.0"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.33.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.12.0",
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.17.1",
    "@types/uuid": "^10.0.0",
    "@unocss/eslint-plugin": "^66.4.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.4.0",
    "gh-pages": "^6.3.0",
    "npm-check-updates": "^17.1.18",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "unocss": "^66.4.2",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
